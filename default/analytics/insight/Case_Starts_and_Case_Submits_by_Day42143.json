{
  "$id": "c_Case_Starts_and_Case_Submits_by_Day42143",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Case Submissions and Deflections by Day",
  "description": "Case Submissions and Deflections by Day",
  "query": {
    "metrics": [
      "yext/ANSWERS_CASE_DEFLECTIONS",
      "yext/ANSWERS_CASE_SUBMITS"
    ],
    "dimensions": [
      "yext/DAYS"
    ],
    "filters": [

      {
        "filter": "yext/ANSWERS_TRAFFIC_TYPE",
        "in": [
          "EXTERNAL"
        ]
      }
    ]
  },
  "display": {
    "bar": {
      "style": "PERCENT"
    }
  }
}