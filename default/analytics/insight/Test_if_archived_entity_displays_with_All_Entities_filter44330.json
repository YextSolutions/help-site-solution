{
  "$id": "c_Test_if_archived_entity_displays_with_All_Entities_filter44330",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Test if archived entity displays with All Entities filter",
  "description": "Answers Searches and Answers Clicks by Entity",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES",
      "yext/ANSWERS_CLICKS"
    ],
    "fields": [
      "entityId",
      "name",
      "address",
      "yext/derivedField-postalCode",
      "yext/derivedField-city"
    ],
    "dimensions": [
      "yext/ENTITIES"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_30"
      },
      {
        "filter": "yext/ANSWERS_SEARCH_ID",
        "in": [
          "e5bb6184-e21a-4d67-be53-2b1e866c6bd6"
        ]
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_CLICKS"
        },
        "descending": true
      }
    }
  }
}