{
  "$id": "c_Search_Terms_That_Led_To_Case_Submission",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Search Terms That Led to Case Submission",
  "description": "Last Search Term run before ticket was submitted.",
  "query": {
    "metrics": [
      "yext/ANSWERS_CASE_SUBMITS"
    ],
    "dimensions": [
      "yext/ANSWERS_SEARCH_TERM"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_14"
      },

      {
        "filter": "yext/ANSWERS_TRAFFIC_TYPE",
        "in": [
          "EXTERNAL"
        ]
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_CASE_SUBMITS"
        },
        "descending": true
      }
    }
  }
}