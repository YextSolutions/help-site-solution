{
  "$id": "c_hero_line_chart_case_deflections",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Support Search Overview",
  "description": "Overview of how your Support Search is performing based on the four key support metrics.",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES",
      "yext/ANSWERS_CASE_STARTS",
      "yext/ANSWERS_CASE_SUBMITS",
      "yext/ANSWERS_CASE_DEFLECTIONS"
    ],
    "dimensions": [
      "yext/DAYS"
    ],
    "filters": [

      {
        "filter": "yext/ANSWERS_TRAFFIC_TYPE",
        "in": [
          "EXTERNAL"
        ]
      }
    ]
  },
  "display": {
    "line": {
      "heroNumbers": {
        "show": true
      }
    }
  }
}